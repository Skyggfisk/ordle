{
  "name": "ordle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:app": "vite build",
    "build:assets": "bun scripts/generateAssetCacheList.ts",
    "build:sw": "bun build src/service-worker.ts --outdir ./dist --target browser --minify",
    "build": "bun run build:app && bun run build:assets && bun run build:sw",
    "check": "bun run lint && bun run typecheck",
    "dev": "vite",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "generate:words": "bun scripts/extractWords.ts",
    "generate:words:dry": "bun scripts/extractWords.ts --dry-run",
    "prepare": "husky"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@happy-dom/global-registrator": "^18.0.1",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.21",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.2.22",
    "commander": "^14.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.5.0"
  }
}
